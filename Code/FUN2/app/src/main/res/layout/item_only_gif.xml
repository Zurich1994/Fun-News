<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="#ffffff">

<alexhao.fun.View.RoundImgView
    android:id="@+id/touxiang9"
    android:layout_width="50dp"
    android:layout_height="50dp"
    android:layout_marginLeft="15dp"
    android:layout_marginTop="15dp"
    android:src="@drawable/ic_launcher" />

<TextView
    android:id="@+id/name9"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignTop="@+id/touxiang9"
    android:layout_marginLeft="10dp"
    android:layout_toRightOf="@+id/touxiang9"
    android:text="alex hao"
    android:textSize="16sp" />

<TextView
    android:id="@+id/time9"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignBottom="@+id/touxiang9"
    android:layout_marginLeft="10dp"
    android:layout_toEndOf="@+id/name9"
    android:layout_toRightOf="@+id/touxiang9"
    android:text="2015-04-01 20:33"
    android:textSize="15sp" />

<ImageButton
    android:id="@+id/more9"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentEnd="true"
    android:layout_alignParentRight="true"
    android:layout_alignTop="@+id/touxiang9"
    android:layout_marginRight="30dp"
    android:background="#00000000"
    android:src="@drawable/more" />

<pl.droidsonroids.gif.GifImageView
    android:id="@+id/img9"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentLeft="true"
    android:layout_below="@+id/time9"
    android:layout_centerHorizontal="true"
    android:layout_marginLeft="15dp"
    android:layout_marginRight="10dp"
    android:layout_marginTop="10dp"
    android:scaleType="centerInside"
     />

<ImageView
    android:id="@+id/nature9"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentEnd="true"
    android:layout_alignParentRight="true"
    android:layout_below="@+id/img9"
    android:layout_marginRight="115dp"
    android:layout_marginTop="10dp"
    android:src="@drawable/nature" />

<TextView
    android:id="@+id/tag9_1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignBottom="@+id/nature9"
    android:layout_alignParentEnd="true"
    android:layout_alignParentRight="true"
    android:layout_marginRight="85dp"
    android:text="搞笑"

    />

<TextView
    android:id="@+id/tag9_2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignBottom="@+id/nature9"
    android:layout_alignParentEnd="true"
    android:layout_alignParentRight="true"
    android:layout_marginRight="50dp"
    android:text="搞笑" />

<TextView
    android:id="@+id/tag9_3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignBottom="@+id/nature9"
    android:layout_alignParentEnd="true"
    android:layout_alignParentRight="true"
    android:layout_marginRight="15dp"
    android:text="搞笑" />

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_below="@+id/tag9_1"
    android:layout_marginBottom="10dp"
    android:layout_marginTop="10dp"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nature9"
        android:layout_weight="1"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_weight="0.9"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/zan9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="0dp"
                android:background="#00000000"
                android:src="@drawable/zan" />

            <TextView
                android:id="@+id/zancount9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"

                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@+id/zan9"
                android:singleLine="true"
                android:text="10000" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.1">

            <ImageView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:src="@drawable/shuxian" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nature9"
        android:layout_weight="1"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_weight="0.9"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/cai9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="0dp"
                android:background="#00000000"
                android:src="@drawable/cai" />

            <TextView
                android:id="@+id/caicount9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"

                android:layout_toRightOf="@+id/cai9"
                android:singleLine="true"
                android:text="10000" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.1">

            <ImageView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:src="@drawable/shuxian" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nature9"
        android:layout_weight="1"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_weight="0.9"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/pinglun9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="0dp"
                android:background="#00000000"
                android:src="@drawable/pinglun" />

            <TextView
                android:id="@+id/plcount9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@+id/pinglun9"
                android:singleLine="true"
                android:text="10000"

                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.1">

            <ImageView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:src="@drawable/shuxian" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nature9"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/share9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="0dp"
            android:background="#00000000"
            android:src="@drawable/share" />

        <TextView
            android:id="@+id/sharecount9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="10dp"
            android:layout_toRightOf="@+id/share9"
            android:singleLine="true"
            android:text="10000" />

    </LinearLayout>
</LinearLayout>

</RelativeLayout>